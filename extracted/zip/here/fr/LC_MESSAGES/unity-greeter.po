#: ../src/user-list.vala:785
msgid "Log In"
msgstr "Se connecter"

#: ../src/user-list.vala:786
#, c-format
msgid "Login as %s"
msgstr "Se connecter en tant que %s"

#: ../src/user-list.vala:790
msgid "Retry"
msgstr "Réessayer"

#: ../src/user-list.vala:791
#, c-format
msgid "Retry as %s"
msgstr "Réessayer en tant que %s"

#: ../src/user-list.vala:829
msgid "Login"
msgstr "Ouvrir la session"

