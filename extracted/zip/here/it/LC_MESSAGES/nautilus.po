msgid "Search"
msgstr "Cerca"

#: ../libnautilus-private/nautilus-query.c:211
#, c-format
msgid "Search for “%s”"
msgstr "Cerca «%s»"

#: ../libnautilus-private/nautilus-search-engine.c:190
msgid "Unable to complete the requested search"
msgstr "Impossibile completare la ricerca indicata"

