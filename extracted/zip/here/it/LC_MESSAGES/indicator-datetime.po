msgid "Date & Time Settings…"
msgstr "Impostazioni data e ora..."

#: ../src/service.c:1262
msgid "Time & Date settings…"
msgstr "Impostazioni Ora & data..."

#. TRANSLATORS: This is a format string passed to strftime to combine the
#. * date and the time.  The value of "%s\xE2\x80\x82%s" will result in a
#. * string like this in US English 12-hour time: 'Fri Jul 16 11:50 AM'.
#. * The space in between date and time is a Unicode en space
#. * (E28082 in UTF-8 hex).
#: ../src/utils.c:177
#, c-format
msgid "%s %s"
msgstr "%s %s"

#: ../src/utils.c:181 ../src/utils.c:185
#, c-format
msgid "%s"
msgstr "%s"

#: ../src/utils.c:280
msgid "Tomorrow"
msgstr "Domani"

#. a strftime(3) fmt string for abbreviated day of week
#. TRANSLATORS: a strftime(3) format showing the weekday
#: ../src/utils.c:285 ../src/utils.c:348
#, c-format
msgid "%a"
msgstr "%a"

#. a strftime(3) fmt string for day-of-month and abbreviated month
#: ../src/utils.c:290
msgid "%d %b"
msgstr "%d %b"

#. a strftime(3) fmt string for a H:MM 12 hour time, eg "6:59 PM"
#. TRANSLATORS: a strftime(3) format for 12hr time
#: ../src/utils.c:301 ../src/utils.c:395
msgid "%l:%M %p"
msgstr "%l.%M %p"

#. a strftime(3) fmt string for a 12 hour on-the-hour time, eg "7 PM"
#: ../src/utils.c:316
msgid "%l %p"
msgstr "%l %p"

#. TRANSLATORS: a strftime(3) format showing the date and weekday
#: ../src/utils.c:342
msgid "%a %b %e"
msgstr "%a %e %b"

#. TRANSLATORS: a strftime(3) format showing the date
#: ../src/utils.c:345
msgid "%b %e"
msgstr "%e %b"

#. TRANSLATORS: a strftime(3) format for 12hr time w/seconds
#: ../src/utils.c:392
msgid "%l:%M:%S %p"
msgstr "%l.%M.%S %p"

#. TRANSLATORS: a strftime(3) format for 24hr time w/seconds
#: ../src/utils.c:398
msgid "%H:%M:%S"
msgstr "%k.%M.%S"

#. TRANSLATORS: a strftime(3) format for 24hr time
#: ../src/utils.c:401
msgid "%H:%M"
msgstr "%k.%M"

